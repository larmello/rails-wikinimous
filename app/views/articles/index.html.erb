<div class="container">
<h1>Welcome to WikiNimous</h1>
 <div class="">
  <article>
    <% @articles.each do |article| %>
      <div class="border border-dark p-2 m-3">
        <h2><strong> <%= link_to article.title, article_path(article) %></strong></h1>
        <div class="p-2 mb-2 bg-secondary text-white">
          <%= article.content %>
        </div>
      </div>
    <% end %>
  </article>
 </div>
</div>
